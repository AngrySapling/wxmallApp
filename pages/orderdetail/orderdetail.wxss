/* pages/orderdetail/orderdetail.wxss */
.detail{
  background: #ecedf2;
  padding: 0 20rpx;
  overflow: auto;
}
.payStatus{
  background: #fff;
  margin-top: 10rpx;
}

.payStatus .img{
  height: 46rpx;
  width: 46rpx;
}
.payStatus .text{
  font-size: 26rpx;
  color: red;
  padding-left: 10rpx;
}
.status{
  height: 46rpx;
  line-height: 46rpx;
  padding: 22rpx 30rpx;
}
.bj{
  width: 100%;
  height: 8rpx;
}
.payStatus .bj image{
  height:100%;
  width: 100%;
  vertical-align: top;
}
#orderdetail .pickup{
  margin-top: -30px !important;
  padding: 12rpx 32rpx;
  background: #fff;
  width: 94%;
  margin: 0 auto;
  border-radius: 10rpx;
  box-sizing: border-box;
}
#orderdetail .pickup .pickup-title .text2,
#orderdetail .pickup .pickup-title .text1{
  flex: 1
}
#orderdetail .pickup .pickup-title .text2{
  text-align: right;
  color: #ffcf6c;
  
}
#orderdetail .pickup .pickup-title{
  width: 100%;
  display: flex;
  height: 45px;
  line-height: 45px;
  font-size: 30rpx;
  border-bottom: 1px solid #ccc;
  box-sizing: border-box;
}
#orderdetail .pickup-nav{
  display: flex;
}
#orderdetail  .pickup-nav{
  padding: 12rpx 32rpx;
}